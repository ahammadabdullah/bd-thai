generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mongodb"
    url      = env("DATABASE_URL")
}

model User {
    id       String  @id @default(auto()) @map("_id") @db.ObjectId
    email    String  @unique
    password String
    role     Role    @default(USER)
    name     String
    agent    String?
}

enum Role {
    ADMIN
    USER
}
