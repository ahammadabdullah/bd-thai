generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mongodb"
    url      = env("DATABASE_URL")
}

model Blog {
    id          String   @id @default(auto()) @map("_id") @db.ObjectId
    slug        String
    title       String
    date        DateTime
    author      String
    readTime    String
    description String
    content     String
    image       String
}
